public class LinkedList{
    Node head;
    Node tail;
    Integer size;
    
    public LinkedList() {
        this.tail = null;
        this.head = null;
        this.size = 0;
    }
    public void addToFront(Integer value){
        
        Node newNode = new Node(value);
        this.size++;

        if (this.head == null)
        {
            this.head = newNode;
            this.tail = this.head;
            return;
        }
        newNode.next = this.head;
        this.head = newNode;
    }
    public Integer removeFromFront(){
        
        if (this.size <1) return null;

        Integer oldHeadData = this.head.data;
        this.head = this.head.next;
        this.size--;

        return oldHeadData;
    }
    public void addToTail(Integer value){

        Node newNode = new Node(value);

        this.size++;

        if (this.tail == null) {
            this.tail = newNode;
            this.head = this.tail;
            return;
        }
        newNode.next = this.head;
        this.tail.next = newNode;
        this.tail = newNode;
        
    }
    public Integer removeFromTail(){

        if (this.size < 1) return null;

        this.size--;

        Integer oldTailData = tail.data;

        if (this.size == 1)
        {
            this.head = null;
            this.tail = null;
            this.size = 0;
            return oldTailData;
        }

        Node penultimateNode = null;

        for (Integer i = 1; i <= this.size-1; i++)
        {
          if (i == this.size-1)
          {
              break;
          }
          penultimateNode = penultimateNode.next;
        }

        this.tail = penultimateNode;
        return oldTailData;
    }
    public Integer size(){ return this.size; }    
}

public class Node{
    Integer data;
    Node next;

    public Node(Integer data) {
        this.data = data;
        this.next = null;
    }
    public Node(Integer data, Node next) {
        this.data = data;
        this.next = next;
    }
}