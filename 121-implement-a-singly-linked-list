public class LinkedList{
    Node head;
    Node tail;
    Integer size;
    
    public LinkedList() {
        this.tail = null;
        this.head = null;
        this.size = 0;
    }
    public void addToFront(Integer value){
        
        Node newNode = new Node(value);
        if (this.head == null)
        {
            this.head = newNode;
            this.tail = this.head;
            return;
        }
        newNode.next = this.head;
        this.head = newNode;
    }
    public Integer removeFromFront(){}
    public void addToTail(Integer value){}
    public Integer removeFromTail(){}
    public Integer size(){}    
}

public class Node{
    Integer data;
    Node next;

    public Node(Integer data) {
        this.data = data;
        this.next = null;
    }
    public Node(Integer data, Node next) {
        this.data = data;
        this.next = next;
    }
}